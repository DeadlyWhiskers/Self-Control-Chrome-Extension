{
    "name": "Self-Control Chrome Extension",
    "description": "End your procrastination yourself!",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "storage"
    ],
    "background":{
      "service_worker": "service_worker.js",
      "type": "module"
    }
  }